<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class catalog="hospital" name="Modelo.Medico" optimistic-lock="version" table="medicos">
        <id name="idMedico" type="java.lang.Integer">
            <column name="idMedico"/>
            <generator class="increment"/>
            <!--<generator class="identity"/>-->
        </id>
        <property name="apellido" type="string">
            <column length="30" name="apellido" not-null="true"/>
        </property>
        <property name="nombre" type="string">
            <column length="30" name="nombre" not-null="true"/>
        </property>

        <property name="consultorio" type="string">
            <column length="10" name="consultorio" not-null="true"/>
        </property>
        <property name="especialidad" type="string">
            <column length="100" name="especialidad" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>

